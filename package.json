{
  "name": "weblueth",
  "version": "0.0.0",
  "description": "My first react typescript package",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jp-96/weblueth.git"
  },
  "targets": {
    "pages": {
      "source": "pages/index.html",
      "isLibrary": false
    }
  },
  "scripts": {
    "build": "yarn build:esm && yarn build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "prepare": "npm run build",
    "start:pages": "npx parcel --no-cache --target pages",
    "build:pages": "npx parcel build --no-cache --no-source-maps --target pages"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "react",
    "typescript",
    "web bluetooth",
    "state machine"
  ],
  "author": "weblueth",
  "license": "MIT",
  "devDependencies": {
    "process": "^0.11.10",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@types/react": "^18.0.28",
    "@types/web-bluetooth": "~0.0.16",
    "@xstate/react": "^3.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "xstate": "^4.37.0"
  }
}
